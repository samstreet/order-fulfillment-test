parameters:
    level: 9
    paths:
        - app
        - config
        - database
        - routes

    # Exclude certain paths
    excludePaths:
        - bootstrap
        - storage
        - vendor
        - node_modules

    # Ignore common Laravel patterns
    ignoreErrors:
        - '#Call to function array_filter\(\) requires parameter \#2 to be passed#'

    # Report unused private elements
    reportUnmatchedIgnoredErrors: true
